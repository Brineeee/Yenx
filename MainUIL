local Gui = game.CoreGui

for _,u in pairs(Gui:GetChildren()) do
    if u.Name == "YenxUltimate.lua" then
         u:Destroy() 
    end
end

local Lib = {
    Name = "YenxUltimate.lua",
    Core = Gui, 
    Size = UDim2.new(0,540,0,398),
    Drag = true, 
    Page = true,
    Scroll = 0
} 

local Color = {
    Text = TextColor or Color3.fromRGB(120,255,0), 
    Title = TitleColor or Color3.fromRGB(120,255,0), 
    Image = ImageColor or Color3.fromRGB(120,255,0), 
    Main = MainColor or Color3.fromRGB(120,255,0),
    Shadow = ShadowColor or Color3.fromRGB(120,255,0)
}

local Info = { 
    Name = game.Players.LocalPlayer.Name, 
    ID = game.Players.LocalPlayer.UserId
}

local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
    
local Library = {}
function Library:Notification(title, desc, t)
for _,w in pairs(game.CoreGui:GetChildren()) do
    if w.Name == "Notification.html" then
        w:Destroy()
    end
end

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextButton1 = Instance.new("TextLabel")
local TextButton12 = Instance.new("TextLabel") 

ScreenGui.Name = "Notification.html" 
ScreenGui.Parent = Gui

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.new(0,0,0)
Frame.BorderColor3 = Color.Main
Frame.Position = UDim2.new(0,-9500,0,365)
Frame.Size = UDim2.new(0,300,0,100)
Frame.Active = true

TextButton1.Parent = Frame
TextButton1.BackgroundColor3 = Color3.fromRGB(0,0,0)
TextButton1.BorderSizePixel = 0
TextButton1.BackgroundTransparency = 0
TextButton1.Position = UDim2.new(0,70.0,0,0)
TextButton1.Size = UDim2.new(0.5,0,0.2)
TextButton1.Font = Enum.Font.Gotham
TextButton1.Text = title
TextButton1.TextColor3 = Color.Text
TextButton1.TextScaled = true
TextButton1.TextSize = 8
TextButton1.TextWrapped = true

TextButton12.Parent = Frame
TextButton12.BackgroundColor3 = Color3.fromRGB(0,0,0)
TextButton12.BorderSizePixel = 0
TextButton12.BackgroundTransparency = 0
TextButton12.Position = UDim2.new(0,20.0,0,20)
TextButton12.Size = UDim2.new(0,260,0,50)
TextButton12.Font = Enum.Font.Code
TextButton12.Text = desc
TextButton12.TextColor3 = Color.Text
TextButton12.TextSize = 14
TextButton12.TextWrapped = true

TweenService:Create(Frame, TweenInfo.new(.7, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Position = UDim2.new(0,0,0,365)}):Play()
wait(t)
TweenService:Create(Frame, TweenInfo.new(.7, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Position = UDim2.new(0,-900,0,365)}):Play() 
wait(.7)
ScreenGui:Destroy() 
end

